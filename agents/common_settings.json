{
  "env": {
    "available_keys": ["AI_API_KEY"],
    "path": ".env"
  },

  "ai_api": {
    "body": {
      "default": {
        "model": "google/gemini-2.5-flash:online",
        "temperature": 0.7,
        "max_tokens": 3000,
        "messages": [
          {
            "role": "system",
            "content": "{SYSTEM_CONTENT}"
          },
          {
            "role": "user",
            "content": "{USER_CONTENT}"
          }
        ],
        "stream": false,
        "top_p": 1
      },
      "specific": {
        "diversifier": {
          "raw": {
            "max_tokens": 20000
          },
          "roles": ["system", "user"]
        },
        "recipes": {
          "raw": {
            "max_tokens": 100000
          },
          "roles": ["system", "user"]
        }
      }
    },
    "urls": {
      "base_api_url": "https://ai.hackclub.com/",
      "chat_completion_endpoint": "proxy/v1/chat/completions",
      "available_models_endpoint": "proxy/v1/models"
    },
    "available_models": [
      "qwen/qwen3-32b",
      "moonshotai/kimi-k2-thinking",
      "openai/gpt-oss-120b",
      "moonshotai/kimi-k2-0905",
      "qwen/qwen3-vl-235b-a22b-instruct",
      "x-ai/grok-4.1-fast",
      "nvidia/nemotron-nano-12b-v2-vl",
      "google/gemini-2.5-flash:online",
      "openai/gpt-5.1",
      "openai/gpt-5-mini",
      "deepseek/deepseek-v3.2-exp",
      "deepseek/deepseek-r1-0528",
      "z-ai/glm-4.6",
      "google/gemini-2.5-flash-image",
      "qwen/qwen3-235b-a22b",
      "qwen/qwen3-235b-a22b-instruct-2507",
      "deepseek/deepseek-r1-distill-qwen-32b",
      "meta-llama/llama-guard-4-12b",
      "google/gemini-3-pro-preview",
      "google/gemini-3-pro-image-preview"
    ],

    "default_headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer {AI_API_KEY}"
    }
  },
  "agents": {
    "available_agents": ["diversifier", "recipes"]
  }
}
